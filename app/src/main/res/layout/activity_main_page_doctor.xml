<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/kabackground"
    tools:context=".MainPageDoctor">

    <View
        android:id="@+id/view15"
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:background="#0099ff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view16"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/ic_wpwwave"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view15" />

    <View
        android:id="@+id/view17"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:background="#0099ff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/view18"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/ic_wpwwave"
        android:rotationX="180"
        app:layout_constraintBottom_toTopOf="@+id/view17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <RelativeLayout
        android:layout_width="350dp"
        android:layout_height="350dp"
        app:layout_constraintBottom_toTopOf="@+id/view18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view16">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/buttonDoctorAppointments1"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/bottom_left_pink"
                android:rotationX="180"
                app:backgroundTint="#0277BD"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/buttonLabReportsDoctor"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:background="@drawable/bottom_left_pink"
                android:rotationX="180"
                android:rotationY="180"
                app:backgroundTint="#FF8F00"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/buttonDoctorConsultancy"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_marginStart="8dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/bottom_left_pink"
                app:backgroundTint="#AD1457"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <Button
                android:id="@+id/button9"
                android:layout_width="160dp"
                android:layout_height="160dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:background="@drawable/bottom_left_pink"
                android:rotationY="180"
                app:backgroundTint="#4527A0"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <Button
                android:id="@+id/button11"
                android:layout_width="159dp"
                android:layout_height="wrap_content"
                android:text="@string/my_appointments"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                app:backgroundTint="#0277BD"
                app:layout_constraintBottom_toBottomOf="@+id/buttonDoctorAppointments1"
                app:layout_constraintEnd_toEndOf="@+id/buttonDoctorAppointments1"
                app:layout_constraintStart_toStartOf="@+id/buttonDoctorAppointments1"
                app:layout_constraintTop_toTopOf="@+id/buttonDoctorAppointments1" />

            <Button
                android:id="@+id/button12"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:text="@string/mylabreports"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                app:backgroundTint="#FF8F00"
                app:layout_constraintBottom_toBottomOf="@+id/buttonLabReportsDoctor"
                app:layout_constraintEnd_toEndOf="@+id/buttonLabReportsDoctor"
                app:layout_constraintStart_toStartOf="@+id/buttonLabReportsDoctor"
                app:layout_constraintTop_toTopOf="@+id/buttonLabReportsDoctor" />

            <Button
                android:id="@+id/button13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/CONSULTANCY"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                app:backgroundTint="#AD1457"
                app:layout_constraintBottom_toBottomOf="@+id/buttonDoctorConsultancy"
                app:layout_constraintEnd_toEndOf="@+id/buttonDoctorConsultancy"
                app:layout_constraintStart_toStartOf="@+id/buttonDoctorConsultancy"
                app:layout_constraintTop_toTopOf="@+id/buttonDoctorConsultancy" />

            <Button
                android:id="@+id/button14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#FFFFFF"
                android:text="@string/contact_us"
                android:textColor="#FFFFFF"
                android:textSize="15sp"
                app:backgroundTint="#4527A0"
                app:layout_constraintBottom_toBottomOf="@+id/button9"
                app:layout_constraintStart_toStartOf="@+id/button9"
                app:layout_constraintTop_toTopOf="@+id/button9" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/medicalteam"
        app:layout_constraintEnd_toEndOf="@+id/view15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewHelloDr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView23" />

    <TextView
        android:id="@+id/textViewHowArYouDr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view15" />
</androidx.constraintlayout.widget.ConstraintLayout>