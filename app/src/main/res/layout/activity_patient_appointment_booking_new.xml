<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1A446B"
    tools:context=".Patient_Appointment_Booking_New">

    <View
        android:id="@+id/view41"
        android:layout_width="match_parent"
        android:layout_height="670dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/divider29"
        android:layout_width="340dp"
        android:layout_height="5dp"
        android:background="@drawable/rounded_square_new"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewUserName_new" />

    <Button
        android:id="@+id/button_Add_New_Appointment_New"
        android:layout_width="340dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/navy"
        android:drawablePadding="0dp"
        android:includeFontPadding="false"
        android:text="@string/newAppointment"
        android:textColor="#FFFFFF"
        android:textSize="19sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/ic_baseline_add_24"
        android:elevation="2dp"
        app:layout_constraintStart_toStartOf="@+id/button_Add_New_Appointment_New"
        app:layout_constraintTop_toTopOf="@+id/button_Add_New_Appointment_New" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewPatientAppointments"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/button_Add_New_Appointment_New"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view41" />

    <TextView
        android:id="@+id/textViewUserName_new"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:paddingTop="7dp"
        android:textAlignment="center"
        android:textColor="#1A446B"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view41" />

    <View
        android:id="@+id/divider30"
        android:layout_width="340dp"
        android:layout_height="5dp"
        android:background="@drawable/rounded_square_new"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recyclerViewPatientAppointments" />
</androidx.constraintlayout.widget.ConstraintLayout>